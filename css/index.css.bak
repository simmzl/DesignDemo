
.page{
    width: 1600px;
    margin: 0 auto;
    zoom: 0.85;
}
a{
    color: #FFFFFF;
}
.fl{
    float: left;
}
.fr{
    float: right;
 }

.nav{
    height: 80px;
    /*80*/
    background-color:#000000;
    margin: 0 auto;
    width: 100%;
}
.nav_logo{
    margin-left: 215px;
    margin-right: 135px;
    /*215 135*/
    font-size: 25px;
    /*25*/
    color: #FFFFFF;
    line-height: 80px;
}
.nav_logo a{
    color: #FFFFFF;
}
.nav_nav ul li{
    float: left;
    color: #FFFFFF;
    line-height: 80px;
    font-size: 16px;
    /*16*/
    margin-right:39px ;
    /*39*/
}
.nav_sign{
    margin-right: 215px;
    /*215;*/
    line-height: 80px;
}
.nav_sign_btn{
    width: 90px;
    height: 35px;
    /*90 35-77 30*/
    border-radius: 9px 9px;
    background-color:rgba(0, 0, 0, 0) ;
    border: 3px solid #ca3c3c;
    color: #ca3c3c;
    font-size: 16px;
}
.header{
    height: 820px;
    background-color: rgba(102,102,102,0.5);
}
