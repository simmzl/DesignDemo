.header_play{
    margin: 305px 0 auto 215px;
}
.header_play img{
    width: 130px;
}
.header_nima{
    clear:right;
}
.header_text{
    font-size: 60px;
    color: #FFFFFF;
    margin: 326px 0 0 25px;
}
.header_toplay{
    font-size: 16px;
    color: #ca3c3c;
    margin: -5px 0 0 27px;
}
.red{
    height: 360px;
    background-color: #ca3c3c;
}
.red_l{
    font-size: 72px;
    color: #FFFFFF;
    width: 370px;
    border-bottom: 10px solid white;
    margin: 39px 0 0 215px;
}
.red_r1{
    margin: 0 30px 0 0;
}
.red_r1_t{
    color: #FFFFFF;
    margin: 10px 0 0 0;
}
.red_r1_t1{
  font-size: 12px;
  padding: 0;
}
.red_r1_t2{
    margin-bottom: 15px;
    padding: 0;
}
.r1,.r2,.r3,.r4{
    float: left;
    /*width: 370px;*/
    /*height: 110px;*/
    margin: 50px 0 0 30px;
}
.icon{
    height: 150px;
}
.icon span{
    display:inline-block;
    /*span是块级元素，使用inline-block不换行，可改变宽高*/
    background-image: url("../images/icons.png");
    width: 40px;
    height: 33px;
}
.icon1{
    background-position:0 0;
    margin: 58px 170px 58px 245px;
}
.icon2{
    background-position: 40px 0;
    margin: 58px 170px 58px 0;
}
.icon3{
    background-position: 80px 0;
    margin: 58px 170px 58px 0;
}
.icon4{
    background-position: 120px 0;
    margin: 58px 170px 58px 0;
}
.icon5{
    background-position: 160px 0;
    margin: 58px 170px 58px 0;
}
.icon6{
    background-position: 200px 0;
    margin: 58px 0 58px 0;
}
.works{
    display: inline-block;
    /*这里一定要有inline-block*/
    height: 1325px;
    background-color: #EBEBEB;
    position:relative;
}
.works_l{
    font-size: 72px;
    color:#000000;
    width: 370px;
    border-bottom: 10px solid black;
    /*padding: 39px 0 0 0;*/
    margin: 39px 0 0 215px;
}
.works_btns{
    margin: 30px 0 0 215px;
}
.works_btn{
    width: 90px;
    height: 35px;
    /*90 35-77 30*/
    border-radius: 9px 9px;
    background-color:rgba(0, 0, 0, 0) ;
    border: 3px solid #ca3c3c;
    color: #ca3c3c;
    font-size: 16px;
    margin-right: 10px;
}
.works_btn:hover{
    background-color: #ca3c3c;
    color: #FFFFFF;
}

.works_imgs_alt{
    margin: 60px 0 0 215px;
}
.works_imgs_alt p{
    background-color: white;
    width: 370px;
    height: 100px;
    float: left;
    margin: 270px 30px 30px 0;
    padding: 0;
    text-align: center;
}
.works_imgs_alt_n{
    font-size: 18px;
    color: #ca3c3c;
    line-height: 60px;
}
.works_imgs_alt_t{
    font-size: 14px;
    color: gray;
}

.works_imgs{
    margin: 60px 0 0 215px;
    position: absolute;
}
.works_imgs p{
    background-color: gray;
    width: 370px;
    height: 370px;
    float: left;
    margin: 0 30px 30px 0;
    transition:height 0.7s;
    -moz-transition:height 0.7s; /* Firefox 4 */
    -webkit-transition:height 0.7s; /* Safari and Chrome */
    -o-transition:height 0.7s; /* Opera */
}
.clear{
    clear: left;
}
.works_imgs p:hover{
    height: 270px;
    width: 370px;
    color: #FFFFFF;
}
.person{
    height: 350px;
    background-color: #666666;
    text-align: center;
    color: #FFFFFF;
    font-size: 36px;
}
.person_text{
    display: inline-block;
    width: 770px;
    margin: 54px auto 17px auto;
}
.person_img{
    background-color: #FFFFFF;
    width: 70px;
    height: 70px;
    border-radius: 35px 35px;
    margin: 0 auto;
}
.team{
    height: 915px;
}
.team_l{
    font-size: 72px;
    color:#000000;
    width: 370px;
    border-bottom: 10px solid black;
    margin: 39px 0 0 215px;
}
.team_o{
    margin: 80px 0 0 215px;
    display: inline-block;
}
.team_o1{
background-color: gray;
width: 170px;
height: 170px;
border-radius: 85px;
/*display:inline;*/
}
.soc{
    position: relative;
}
.soc s{
    background-image: url("../images/tfi.png");
    display:inline-block;
    position: absolute;
    width: 50px;
    height: 50px;
}
.soc1{
    background-position: 0 0;
    margin: 60px 0 0 140px;
}
.soc2{
    background-position: 110px 0;
    margin: 60px 0 0 200px;
}
.soc3{
    background-position: 50px 0;
    margin: 60px 0 0 260px;
}
.team_o_t{
    margin: 45px 0 0 100px;
}
.team_o_t_n{
    color: #ca3c3c;
    font-size: 18px;
}
.team_o_t_j{
    color: #666666;
    font-size: 14px;
    margin-top: 10px;
}
.team_o_t_alt{
    font-size: 14px;
    width: 300px;
    margin-top: 20px;
}
/*定义多个类要用,逗号，切记*/
.team_t,.team_th{
    margin: 80px 0 0 30px;
    display: inline-block;
}
.more{
    clear: left;
    float: left;
    margin: 30px 0 0 215px;
}


